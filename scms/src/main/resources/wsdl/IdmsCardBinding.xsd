	<!-- ExtractImageAsJpegRequest Element Defined -->
	<xs:element name="ExtractImageAsJpegRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		IDMS to extract the image data from a PIV card facial image
        		CBEFF, convert the image to JPEG if it is encoded as
        		JPEG2000 and then return the JPEG image data to the caller. 
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="CardHolderFacialImageCbeff" 
		        		type="xs:base64Binary" minOccurs="0" >
