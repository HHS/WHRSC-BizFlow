<!-- PrepareBindExternalCardRequest Element Defined -->
	<xs:element name="PrepareBindExternalCardRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		IDMS to begin the card binding process.  This method will 
        		parse the given card data containers, capture the extracted 
        		information, validate the extracted information and check
        		to insure that the relevent business are being followed.
        		The information extracted from the card along with the 
        		results of the data and business rule validation are
        		returned to the caller.
        		If this method returns SUCCESS, the bind operation may be
        		completed by calling the CommitBindExternalCardRequest.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="WantCardHolderFacialImageJpeg" 
						type="xs:boolean" minOccurs="0" default="false">
					<xs:annotation>
						<xs:documentation>
							If this field is present and set to true and 
							a valid card holder facial image is included 
							in this request, then the card holder facial image 
							will be returned in JPEG format.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ApplicantPI" 
						type="PersonIdType" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							This field should be populated with the HHS-ID of the
							person whose external card is to  be bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SubmitterPI" 
						type="PersonIdType" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							This field should be populated with the HHS-ID of the
							person who is submitting the bind request.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EwsResourceTag" 
						type="ResourceTagType" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							This field should be populated with the resource tag 
							of the enrollment workstation that is submitting the 
							bind request.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EwsSiteTag" 
						type="SiteTagType" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							This field should be populated with the site tag of
							the site where the enrollment workstation that is 
							submitting the bind request is located.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				
				<xs:element name="CardCIN" minOccurs="0" maxOccurs="1">
	                <xs:annotation>
	                	<xs:documentation>
	                		The card Chip ID Number.  Note that this is not a PIV
	                		data model element.
	                	</xs:documentation>
	                </xs:annotation>
	                <xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50"></xs:maxLength>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				
				<xs:element name="CardATR" minOccurs="0" maxOccurs="1">
	                <xs:annotation>
	                	<xs:documentation>
	                		The result of card Acknowledgment To Reset.  Note 
	                		that this is not a PIV data model element.
	                	</xs:documentation>
	                </xs:annotation>
	                <xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="66"></xs:maxLength>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				
				<xs:element name="CardIIN" minOccurs="0" maxOccurs="1">
	                <xs:annotation>
	                	<xs:documentation>
	                		The card Issuer ID Number.  Note that this is not a PIV
	                		data model element.
	                	</xs:documentation>
	                </xs:annotation>
	                <xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="15"></xs:maxLength>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			
				<xs:element name="CardCapabilityContainerCont" 
						type="xs:base64Binary" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							Populated with the Card Capability Container 
							object on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CardHolderUniqueIdCont"
						type="xs:base64Binary" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
						    Populated with the Card Holder Unique Identifier 
						    container on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SecurityObjectCont"
						type="xs:base64Binary" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
						    Populated with the Security Object container 
						    on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DiscoveryObjectCont"
						type="xs:base64Binary" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
						    Populated with the Discovery Object container 
						    on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="KeyHistoryObjectCont"
						type="xs:base64Binary" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
						    Populated with the Key History Object container 
						    on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
		        <xs:element name="CardPrintedInfoCont" 
		        		type="xs:base64Binary" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>
							Populated with the Card Printed Information 
							container on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				
				<xs:element name="CardHolderFingerprintsCont" 
		        		type="xs:base64Binary" minOccurs="1" >
					<xs:annotation>
						<xs:documentation>
							Populated with the Cardholder Fingerprints 
							container on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CardHolderFacialImageCont" 
		        		type="xs:base64Binary" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>
							Populated with the Cardholder Facial Image 
							container on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CardHolderIrisImageCont" 
		        		type="xs:base64Binary" minOccurs="0" >
					<xs:annotation>
						<xs:documentation>
							Populated with the Cardholder Iris Image 
							container on the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PivAuthenticationCertCont"
						type="xs:base64Binary" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
						    Populated with the PIV Authentication Certificate
						    container of the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PivDigitalSignatureCertCont"
						type="xs:base64Binary" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Populated with the PIV Digital Signature Certificate
						    container (if present) of the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PivKeyManagementCertCont"
						type="xs:base64Binary" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Populated with the PIV Key Management Certificate
						    container (if present) of the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PivCardAuthenticationCertCont"
						type="xs:base64Binary" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							Populated with the PIV Card Authentication Certificate
						    container (if present) of the PIV card being bound.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
        </xs:complexType>
	</xs:element>



	<!-- PrepareBindExternalCardResponse Element Defined -->
