    <xs:simpleType name="GUIDasStringType">
        <xs:annotation>
            <xs:documentation>
				Globally Unique Identifier - as a string of hexidecimal
				digits
			</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern
				value="([0-9a-fA-F]){32}|([0]){16}">
			</xs:pattern>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="CHUIDDataFieldsType">
        <xs:annotation>
            <xs:documentation>
        		Type definition for Card Holder Unique ID data elements
        	</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FASCN" type="FASCNDataFieldsType" maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Card FASC-N</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GUID" type="GUIDasStringType" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Card GUID</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ExpirationDate" type="xs:date" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
						Card Expiration Date
					</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--  Card History Type -->
    <xs:complexType name="CardHistoryType">
        <xs:annotation>
            <xs:documentation>
        		Type definition for Card History data elements
        	</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CHUID" type="CHUIDDataFieldsType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
						Each CHUID element contains data fields which mirror the data fields 
						in the CHUID of a PIV card.
						If the person specified in the GetCardHistoryRequest
						has any issued cards, one of these elements wil be 
						returned for each card.
					</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssuedDate" type="xs:date" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
						The date the card was issued.
					</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RevokedDate" type="xs:date" maxOccurs="1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
						The date the card was revoked.
					</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Status" type="CardStatusType" maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
						The card status
					</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CardHistoryListType">
        <xs:annotation>
            <xs:documentation>
        		Type definition for a List of Card History data elements
        	</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CardInformation" type="CardHistoryType" maxOccurs="unbounded" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
					Each CardInformation element contains data related to a given instance
					of a card's history. If the person specified in the GetCardHistoryRequest
					has any issued cards, one of these elements will be returned for each card.
				</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- HHS Employer Name Type Defined -->
    <xs:simpleType name="EmployerNameType">
        <xs:annotation>
            <xs:documentation>
				The name of an employer.  Can be the government or the name of a private
				company.
			</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="32"></xs:maxLength>
            <xs:minLength value="1"></xs:minLength>
        </xs:restriction>
    </xs:simpleType>
    <!-- HHS Work Location Code Type Defined -->
    <xs:simpleType name="WorkLocationCodeType">
        <xs:annotation>
            <xs:documentation>
				A code describing the primary work location for a person.
			</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="24"></xs:maxLength>
            <xs:minLength value="1"></xs:minLength>
        </xs:restriction>
    </xs:simpleType>
    <!-- X509CertDerType Defined -->
    <xs:simpleType name="X509CertDerType">
        <xs:restriction base="xs:base64Binary">
            <xs:maxLength value="5000"></xs:maxLength>
        </xs:restriction>
    </xs:simpleType>
    <!-- Commisioned Corps Member Serial NUmber -->
    <xs:simpleType name="CopsSerialNumberType">
        <xs:annotation>
            <xs:documentation>
                A 5 digit employee ID assigneed to Commision Corps 
                members by the COPS system.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{5}"></xs:pattern>
            <xs:length value="5"></xs:length>
        </xs:restriction>
    </xs:simpleType>
    <!-- Commisioned Corps Member assignment Agency, set by COPS -->
    <xs:simpleType name="CopsAgencyType">
        <xs:annotation>
            <xs:documentation>
                The COPS Agency identifer indicating where a Commision
                Corps Member is to be deployed.  The agency identifier
                may be set by the COPS system to allow for auditing and
                consistancy checking of COPS applicants within the SCMS.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="100"></xs:maxLength>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>