            </xs:element>
            <xs:element name="CredentialSeries" maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
						Credential Series
					</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:length value="1"></xs:length>
                        <xs:pattern value="\d{1}"></xs:pattern>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="IndividualCredentialIssue" maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
						Individual Credential Issue
					</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:length value="1"></xs:length>
                        <xs:pattern value="\d{1}"></xs:pattern>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="PersonIdentifier" maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
						Person Identifier
					</xs:documentation>
