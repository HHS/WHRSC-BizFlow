                    <xs:restriction base="xs:string">
                        <xs:length value="1"></xs:length>
                        <xs:enumeration value="1">
                            <xs:annotation>
                                <xs:documentation>Federal Government Agency</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="2">
                            <xs:annotation>
                                <xs:documentation>State Government Agency</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="3">
                            <xs:annotation>
                                <xs:documentation>Commercial Enterprise</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="4">
                            <xs:annotation>
                                <xs:documentation>Foreign Government</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:pattern value="\d{1}"></xs:pattern>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="OrganizationalIdentifier" maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
						Organizational Identifier
					</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:length value="4"></xs:length>
                        <xs:pattern value="\d{4}"></xs:pattern>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="PersonOrganizationAssociation" maxOccurs="1" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
						Person Organization Association
					</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:length value="1"></xs:length>
                        <xs:pattern value="\d"></xs:pattern>
                        <xs:enumeration value="1">
                            <xs:annotation>
                                <xs:documentation>Employee</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="2">
                            <xs:annotation>
                                <xs:documentation>Civil</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="3">
                            <xs:annotation>
                                <xs:documentation>Executive Staff</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="4">
                            <xs:annotation>
                                <xs:documentation>Uniformed Service</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="5">
                            <xs:annotation>
                                <xs:documentation>Contractor</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
