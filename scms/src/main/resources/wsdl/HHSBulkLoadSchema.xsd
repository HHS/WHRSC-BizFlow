                    <xs:restriction base="xs:string">
                        <xs:maxLength value="8"></xs:maxLength>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="CostCenter" minOccurs="0" maxOccurs="1" type="CostCenterType">
                <xs:annotation>
                    <xs:documentation>Cost Center</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CAN" minOccurs="0" maxOccurs="1" type="CANType">
                <xs:annotation>
                    <xs:documentation>CAN</xs:documentation>
                </xs:annotation>
