            <xs:element name="SubAgency" minOccurs="0" maxOccurs="1" type="SubAgencyType">
                <xs:annotation>
                    <xs:documentation>Sub-agency</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FundsCertification" minOccurs="0" maxOccurs="1" type="FundsCertificationType">
                <xs:annotation>
                    <xs:documentation>
                		Funds Certification
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SponsorshipStatus"
        		type="StatusCodeType" minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>
        				Sponsorship Status
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SponsorPI" type="PersonIdType"
        		minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>Sponsor PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SponsorDate" type="xs:date" minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>Sponsor Date</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InductorPI" type="PersonIdType"
        		minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>Inductor PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InductorEmail" minOccurs="0" type="EmailAddressType">
                <xs:annotation>
                    <xs:documentation>Inductor Email</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SACAdjudicationStatus"
        		type="AdjudicationStatusCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				If this field is present it provides the 
        				SAC Adjudication Status. 
        				Possible values are (NA, R, P or C)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SACAdjudicatorPI" type="PersonIdType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>SAC Adjudicator PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SACAdjudicationDate" type="xs:date"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				SAC Adjudication Date
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestAdjudicationStatus"
        		type="AdjudicationStatusCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				If this field is present it provides the 
        				background check Adjudication Status. 
        				Possible values are (NA, R, P or C)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestType"
        		type="PersonInvestigationCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				Type of background investigation
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestAdjudicatorPI" type="PersonIdType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>background check Adjudicator PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestAdjudicationDate" type="xs:date"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				background check Adjudication Date
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PrimarySmtpEmailAddress" minOccurs="0" maxOccurs="1" type="EmailAddressType">
                <xs:annotation>
                    <xs:documentation>
                		Primary Email Address
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UPN" minOccurs="0" maxOccurs="1" type="UPNType">
                <xs:annotation>
                    <xs:documentation>
                		Active Directory UPN
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BadgeExpDate" type="xs:date"
        		minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>
        				Badge expiration date
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssuingAuthority"
        		type="IssuingAuthorityCodeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Issuing Authority. IMPORTANT: This field has been deprecated
                        Any data saved with this field will be ignored by the system
                        If you need to update the issuing authority then update the 
                        OPDIV field. The correct issuing authority will be set
                        based on the entered OPDIV field
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PropertyPassCodes"
        		type="PropertyPassCodeType" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                    <xs:documentation>
        				Property pass codes - Appears in Zone 4 Front PIV (NIH will not use)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecAccessCodes"
        		type="SecurityAccessCodeType" minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>
        				Security access codes - Appears in Zone 5 From PIV (NIH will not use)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SOI" minOccurs="0" type="SOIType">
                <xs:annotation>
                    <xs:documentation>
                		A 4 digit code assigned by OPM signifying a specific 
        				source for an enrollment record and where background 
        				check notifications should be sent.
                        <br/>
        				If set, this value will probably be overridden
						by the enrollment package.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SON" minOccurs="0" type="SONType">
                <xs:annotation>
                    <xs:documentation>
                		This field has different semantics depending on the value of 
                		the PositionSensitivity field.
                        <br/>
                		If PositionSensitivity is 1 and this field is specified,
                		it will supersede the value of the "SON" field in the 
                		enrollment package.
                        <br/>
                		If PositionSensitivity is greater than 1, then this field must
                		be set (becomes mandatory) and will supersede by value of the 
                		"SON" field in the enrollment package.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EnrollmentType" type="EnrollmentTypeCodeType"
        		minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				If this element is specified, it determines if a
        				card will be issued for the person being
        				inducted.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PivProcessFlowCode" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				If this element is specified, it determines which
        				PIV card issuance process flow will be followed.
        				If the element is omitted, the default PIV card
        				issuance process flow will be used.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EarliestCardIssueDate" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				If this element is specified, it determines the
        				earliest date on which the PIV card may be
        				issued.
                        <br/>
        				if this element is not specified, the system issues 
        				the card on the earliest possible date.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AlternateUID" type="ApplicantAlternateUIDType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
        				Alternate user ID (UID) for the Applicant on an
        				external system.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OPAC" minOccurs="0" type="OPACType">
                <xs:annotation>
                    <xs:documentation>
        				The OPAC (Online Payment and Collection Agency Locator Codes)
        				code for this Applicant.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkInvestigateOnly" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                		This field indicates if the person being inducted is only
                		going to have a background investigation and no card
                		is going to be created for the user. IMPORTANT: If this field
                		is checked then do not set any Card Type in the CardTypeCode field
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CardTypeCode" type="CardTypeCodeType"
        		minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Determines the card type associated to the user. If
        				NON_PIV is selected, the Card Association field is required
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CardAssociation" type="CardAssociationType"
        		minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Determines the card type association for card types other than PIV.
        				This field is required only if the card type is other than PIV.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AutoProduceCard" minOccurs="0" type="xs:boolean" default="true">
                <xs:annotation>
                    <xs:documentation>
                		This field indicates if card will be auto produced after
                		the applicant is adjudicated
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObligatingDocumentNumber" minOccurs="0" maxOccurs="1" type="ObligatingDocumentNumberType">
                <xs:annotation>
                    <xs:documentation>Obligating Document Number</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CopsAgency" type="CopsAgencyType" minOccurs="0" 
                    maxOccurs="1" nillable="true">
                <xs:annotation>
                    <xs:documentation>
                        The COPS Agency identifer indicating where a Commision
                        Corps Member is to be deployed.  The agency identifier
                        may be set by the COPS system to allow for auditing and
                        consistancy checking of COPS applicants within the SCMS.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ComponentCode"
        		type="ComponentCodeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Component code (CMS OpDiv Only)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
