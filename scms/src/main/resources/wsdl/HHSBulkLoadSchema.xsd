<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="xsddoc.xsl"?>
<xs:schema xmlns="http://hspd12.hhs.gov/federated/enrollment/v1"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://hspd12.hhs.gov/federated/enrollment/v1"
	xmlns:tns="http://hspd12.hhs.gov/federated/enrollment/v1"
	xmlns:bpd="http://hspd12.bearingpoint.com/webservice/doc"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	id="HHSBulkLoadSchema" version="1.1.2008-07-10">
    <xs:include schemaLocation="HHSCommonTypes.xsd"></xs:include>
    <xs:annotation>
        <xs:appinfo>
            <bpd:schemaDocInclude bpd:file="HHSCommonTypes.xsd"/>
            <bpd:schemaDocInclude bpd:file="CommonEnumerations.xsd"/>
        </xs:appinfo>
    </xs:annotation>
    <!-- HHSInductPersonRequiredDataType Defined -->
    <xs:complexType name="HHSInductPersonRequiredDataType">
        <xs:annotation>
            <xs:documentation>
        		This Type defines the required data items which may be 
        		included in a Induct Person request.
        	</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="PersonSponsored" minOccurs="1" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                		This field indicates if the person being inducted is also
                		sponsored.  If this element is set to "true" then information
                		about the sponsor must also be provided in the 
                		HHSInductPersonOptionalDataType element.
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EmployeeID" type="OpdivEmployeeIDType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's Employee ID as assigned by their
        				OPDIV (this is not a person's PI)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FirstName" minOccurs="1" type="FirstNameType">
                <xs:annotation>
                    <xs:documentation>
                		The person's first name
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LastName" minOccurs="1" type="LastNameType">
                <xs:annotation>
                    <xs:documentation>
                		The person's last name
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PersonID" type="PersonIdType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                		If present, this element sets the PI for the person to be inducted.
                		If this element is omitted, the IDMS will assign a PI to the
                		person being inducted.
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IdentificationInfo" type="IdentificationInformationType"
        		minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				The person's identification information (
        				such as social security number or ARN)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DOB" type="xs:date" minOccurs="1" >
                <xs:annotation>
                    <xs:documentation>
        				The person's date of birth
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CountryOfCitizenship"
        		type="CountryCodeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				The person's country of citizenship (should be
        				NCIC 2000?)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PositionSensitivity" minOccurs="0"
        		type="PositionSensitivityType" maxOccurs="1" default="1">
                <xs:annotation>
                    <xs:documentation>
        				PositionSensitivity
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EmergencyResponder"
        		type="YesNoCodeType" minOccurs="1" >
                <xs:annotation>
                    <xs:documentation>
        				Indicates if person is a federal emergency
        				responder
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Organization" minOccurs="1" type="OrganizationCodeType">
                <xs:annotation>
                    <xs:documentation>
                		The organization sponsoring the person
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OPDIV" type="OpdivCodeType"
        		minOccurs="1" >
                <xs:annotation>
                    <xs:documentation>
        				The OPDIV sponsoring the person
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AffiliationCode"
        		type="AffiliationCodeType" minOccurs="1" >
                <xs:annotation>
                    <xs:documentation>
        				Affiliation code
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CAN" minOccurs="0" maxOccurs="1" type="CANType">
                <xs:annotation>
                    <xs:documentation>CAN</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PrimarySmtpEmailAddress" minOccurs="0" maxOccurs="1" 
        			type="EmailAddressType">
                <xs:annotation>
                    <xs:documentation>
                		Primary Email Address
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UPN" minOccurs="0" maxOccurs="1" type="UPNType">
                <xs:annotation>
                    <xs:documentation>
                		Active Directory UPN
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IssuingAuthority" type="IssuingAuthorityCodeType"
        			 minOccurs="0" maxOccurs="1" >
                <xs:annotation>
                    <xs:documentation>
        				Issuing Authority. IMPORTANT: This field has been deprecated
                        Any data saved with this field will be ignored by the system
                        If you need to update the issuing authority then update the 
                        OPDIV field. The correct issuing authority will be set
                        based on the entered OPDIV field
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CredentialCategory"
        		type="CredentialCategoryType" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Used to determine color bar (Zone 15) on PIV
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ComponentCode"
        		type="ComponentCodeType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Component code (CMS OpDiv Only)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- HHSInductPersonOptionalDataType Defined -->
    <xs:complexType name="HHSInductPersonOptionalDataType">
        <xs:annotation>
            <xs:documentation>
        		This Type defines the optional data items which may be 
        		included in an Induct Person request.
        	</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Prefix" type="NamePrefixType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's name prefix
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MiddleName" maxOccurs="1" minOccurs="0"
        		type="MiddleNameType">
                <xs:annotation>
                    <xs:documentation>
        				The person's middle name
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GenerationQualifier"
        		type="NameGenerationQualifierType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's generational qualifier
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NameSuffix" minOccurs="0"
        		type="NameSuffixType">
                <xs:annotation>
                    <xs:documentation>
        				The person's name suffix such as "Esq."
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CityOfBirth" minOccurs="0"
        		type="CityOfBirthType">
                <xs:annotation>
                    <xs:documentation>
        				The person's city of birth
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="StateOfBirth" type="StateCodeType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's state of birth
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CountryOfBirth" type="CountryCodeType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's country of birth (should be
        				NCIC-2000?)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="VisaNumber" type="VisaNumberType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's Visa number
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ARN" type="AlienRegistrationNumberType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's Alien Registration Number
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ForeignID" type="ForeignNationalIdInfoType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				The person's Foreign National ID information
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PositionTitle" minOccurs="0"
        		type="PositionTitleType">
                <xs:annotation>
                    <xs:documentation>
        				The person's Position Title
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AlternateEmailAddress" minOccurs="0"
        		type="EmailAddressType">
                <xs:annotation>
                    <xs:documentation>
        				Alternate email address
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="WorkPhone" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Work Phone</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="30"></xs:maxLength>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="WorkPhoneExtension" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				Work phone extension
        			</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="8"></xs:maxLength>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="CostCenter" minOccurs="0" type="CostCenterType">
                <xs:annotation>
                    <xs:documentation>Cost Center</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SubAgency" minOccurs="0" type="SubAgencyType">
                <xs:annotation>
                    <xs:documentation>Sub-agency</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FundsCertification" minOccurs="0" type="FundsCertificationType">
                <xs:annotation>
                    <xs:documentation>
                		Funds Certification
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SponsorshipStatus"
        		type="StatusCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				Sponsorship Status
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SponsorPI" type="PersonIdType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Sponsor PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SponsorDate" type="xs:date"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Sponsor Date</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InductorPI" type="PersonIdType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Inductor PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InductorEmail" minOccurs="0"
        		type="EmailAddressType">
                <xs:annotation>
                    <xs:documentation>Inductor Email</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SACAdjudicationStatus"
        		type="AdjudicationStatusCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				If this field is present it provides the 
        				SAC Adjudication Status. 
        				Possible values are (NA, R, P or C)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SACAdjudicatorPI" type="PersonIdType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>SAC Adjudicator PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SACAdjudicationDate" type="xs:date"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				SAC Adjudication Date
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestAdjudicationStatus"
        		type="AdjudicationStatusCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				If this field is present it provides the 
        				background check Adjudication Status. 
        				Possible values are (NA, R, P or C)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestType"
        		type="PersonInvestigationCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				Type of background investigation
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestAdjudicatorPI" type="PersonIdType"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>background check Adjudicator PI</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkinvestAdjudicationDate" type="xs:date"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				background check Adjudication Date
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BadgeExpDate" type="xs:date"
        		minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				Badge expiration date. Note that a default badge
        				expiration date will be calculated by the IDMS
        				if a badge expiration date is not specified.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PropertyPassCodes"
        		type="PropertyPassCodeType" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                    <xs:documentation>
        				Property pass codes - Appears in Zone 4 Front
        				PIV (NIH will not use)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SecAccessCodes"
        		type="SecurityAccessCodeType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
        				Security access codes - Appears in Zone 5 From
        				PIV (NIH will not use)
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SOI" minOccurs="0" type="SOIType">
                <xs:annotation>
                    <xs:documentation>
        				A 4 digit code assigned by OPM signifying a specific 
        				source for an enrollment record and where background 
        				check notifications should be sent.
                        <br/>
        				If set, this value will probably be overridden
						by the enrollment package.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SON" minOccurs="0" type="SONType">
                <xs:annotation>
                    <xs:documentation>
        				This field has different semantics depending on the value of 
                		the PositionSensitivity field.
                        <br/>
                		If PositionSensitivity is 1 and this field is specified,
                		it will supersede the value of the "SON" field in the 
                		enrollment package.
                        <br/>
                		If PositionSensitivity is greater than 1, then this field must
                		be set (becomes mandatory) and will supersede by value of the 
                		"SON" field in the enrollment package.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EnrollmentType" type="EnrollmentTypeCodeType"
        		minOccurs="0" maxOccurs="1" default="PIV-AutoProduceCard">
                <xs:annotation>
                    <xs:documentation>
        				IMPORTANT: This field has been DEPRECATED. It is still
        			    part of the wsdl for backward compatibility. However, it will
        			    be removed from the system in the near future.
        			    Please use the CardTypeCode, BkInvestigateOnly, AutoProduceCard and/or
        			    the CardAssociationType fields to determine the type of enrollment.
                        Also, the request will be rejected if an enrollmentType,
                        CardType, KIkInvestigateOnly, CardAssocationType and/or
                        AutoProduceCard are defined in the same request.
        			    
        			    If this element is specified, it determines if a
        				card will be issued for the person being
        				inducted.
        				If this element is not specified, it defaults to
        				AutoProduceCard.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PivProcessFlowCode" minOccurs="0"
        		maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				If this element is specified, it determines
        				which PIV card issuance process flow will be
        				followed. If the element is omitted, the default
        				PIV card issuance process flow will be used.
        			</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"></xs:minLength>
                        <xs:maxLength value="20"></xs:maxLength>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="EarliestCardIssueDate" type="xs:date"
        		minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				If this element is specified, it determines the
        				earliest date on which the PIV card may be
        				issued.
                        <br/>
        				if this element is not specified, the system issues 
        				the card on the earliest possible date.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AlternateUID" type="ApplicantAlternateUIDType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
        				Alternate user ID (UID) for the Applicant on an
        				external system.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OPAC" minOccurs="0" type="OPACType">
                <xs:annotation>
                    <xs:documentation>
        				The OPAC (Online Payment and Collection Agency Locator Codes)
        				code for this Applicant.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BkInvestigateOnly" minOccurs="0" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>
                		This field indicates if the person being inducted is only
                		going to have a background investigation and no card
                		is going to be created for the user. IMPORTANT: If this field
                		is checked then do not set any Card Type in the CardTypeCode field
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CardTypeCode" type="CardTypeCodeType"
        		minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Determines the card type associated to the user. If
        				NON_PIV is selected, the Card Association field is required
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CardAssociation" type="CardAssociationType"
        		minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				Determines the card type association for card types other than PIV.
        				This field is required only if the card type is other than PIV.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AutoProduceCard" minOccurs="0" type="xs:boolean" default="true">
                <xs:annotation>
                    <xs:documentation>
                		This field indicates if card will be auto produced after
                		the applicant is adjudicated
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ObligatingDocumentNumber" minOccurs="0" maxOccurs="1" type="ObligatingDocumentNumberType">
                <xs:annotation>
                    <xs:documentation>Obligating Document Number</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CopsAgency" type="CopsAgencyType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        The COPS Agency identifer indicating where a Commision
                        Corps Member is to be deployed.  The agency identifier
                        may be set by the COPS system to allow for auditing and
                        consistancy checking of COPS applicants within the SCMS.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- InductionDataType Defined -->
    <xs:complexType name="InductionDataType">
        <xs:annotation>
            <xs:documentation>
        		This Type defines the data items associated with a
        		Induct Person request.
        	</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Prefix" type="NamePrefixType"
        		minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>
        				The person's name prefix
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FirstName" minOccurs="0" maxOccurs="1" type="FirstNameType">
                <xs:annotation>
                    <xs:documentation>
                		The person's first name
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MiddleName" type="MiddleNameType" maxOccurs="1" 
        	       minOccurs="0" nillable="true">
                <xs:annotation>
                    <xs:documentation>
                		The person's middle initial or name.
                		If an UpdateInductionDataRequest is received where this 
                        element contains the xs:nil attribute with the value true,
                        then any existing middle name associated with the Applicant being 
                        updated will be cleared.
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LastName" minOccurs="0" maxOccurs="1" type="LastNameType">
                <xs:annotation>
                    <xs:documentation>
                		The person's last name
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="GenerationQualifier"
        		type="NameGenerationQualifierType" minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>
        				The person's generational qualifier
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NameSuffix" minOccurs="0" type="NameSuffixType">
                <xs:annotation>
                    <xs:documentation>
                		The person's name suffix such as "Esq."
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PersonID" type="PersonIdType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                		If present, this element sets the PI for the person to be inducted.
                		If this element is omitted, the IDMS will assign a PI to the
                		person being inducted.
                	</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EmployeeID" type="OpdivEmployeeIDType" minOccurs="0"
        	       nillable="true">
                <xs:annotation>
                    <xs:documentation>
        				The person's Employee ID as assigned by their
        				OPDIV (this is not a person's PI)
        				If an UpdateInductionDataRequest is received where this 
        				element contains the xs:nil attribute with the value true,
        				then any existing EmployeeID associated with the Applicant being 
        				updated will be cleared.
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SSN" type="SocialSecurityNumberType"
        		minOccurs="0" >
                <xs:annotation>
                    <xs:documentation>
        				The person's social security number
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DOB" type="xs:date" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
        				The person's date of birth
        			</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CityOfBirth" minOccurs="0" type="CityOfBirthType">
