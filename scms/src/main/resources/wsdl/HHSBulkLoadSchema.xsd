                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="WorkPhoneExtension" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                		Work phone extension
                	</xs:documentation>
                </xs:annotation>
