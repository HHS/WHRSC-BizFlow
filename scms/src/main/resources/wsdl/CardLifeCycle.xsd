<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="xsddoc.xsl"?>
<xs:schema xmlns="http://hspd12.hhs.gov/federated/enrollment/v1"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://hspd12.hhs.gov/federated/enrollment/v1"
	xmlns:tns="http://hspd12.hhs.gov/federated/enrollment/v1"
	xmlns:bpd="http://hspd12.bearingpoint.com/webservice/doc"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	id="CardLifeCycle" version="1.0.2007-10-01">

	<xs:include schemaLocation="HHSCommonTypes.xsd"></xs:include>
	<xs:annotation>
		<xs:appinfo>
			<bpd:schemaDocInclude bpd:file="HHSCommonTypes.xsd"/>
		</xs:appinfo>
	</xs:annotation>
	
	<!-- CardTrackingUpdateRequest Element Defined -->
	<xs:element name="CardTrackingUpdateRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		Bulk Induction Web Service of the IDMS in order to allow
        		the BearingPoint IDMS to track card delivery progress.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="TransactionHeader"
						type="TransactionHeaderType"  minOccurs="1">
					<xs:annotation><xs:documentation> 
						The standard IDMS transaction header.
						</xs:documentation></xs:annotation>
				</xs:element>
				<xs:element name="CardBarCodeData" minOccurs="1"
					maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>
							This element contains the data retrieved by
							scanning the bar code on a PIV card whose
							progress is being tracked. There must be one
							or more of these elements present in the
							CardTrackingUpdateRequest.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="256"></xs:maxLength>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PackageTrackingID" maxOccurs="1"
					minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							Package tracking number or other package
							tracking identifier of the package the
							card(s) were received in.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="128"></xs:maxLength>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CardReceivedLocation" type="xs:string"
					maxOccurs="1" minOccurs="1">
                    <xs:annotation>
                    	<xs:documentation>
                    		The location of the facility where the card
                    		was received.
                    	</xs:documentation>
                    </xs:annotation>
				</xs:element>
				<xs:element name="CardHandlerPI" type="PersonIdType" maxOccurs="1" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The PI of the person who is handling the
							receipt and processing of the cards
							referenced in this
							CardTrackingLogisticsRequest.
						</xs:documentation>
					</xs:annotation></xs:element>
			</xs:sequence>
        </xs:complexType>
	</xs:element>


	<!-- CardTrackingUpdateResponse Element Defined -->
	<xs:element name="CardTrackingUpdateResponse">
        <xs:annotation>
        	<xs:documentation>
        		In response to a "CardTrackingUpdateRequest", the IDMS will
        		send this element to the requester so that the results
        		of the request can be retrieved.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="ResultCode" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							A short code describing the results of the
							card tracking update request.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Success">
								<xs:annotation><xs:documentation>
										The card tracking information was successfully processed by the IDMS.
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CardHandlerPiNotFound">
								<xs:annotation><xs:documentation>
										The card holder PI specified in the request was not found
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Failed">
								<xs:annotation><xs:documentation>
										An unexpected error has occurred
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FailureDetailMessage" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							If the ResultCode is "Failed", this field
							may contain some extra information which may
							be helpful in diagnosing the reason for the
							failure. Note that the information in this
							field is not intended to be useful to the
							average user but is intended to help
							advanced users or help desk personal debug
							the problem.
						</xs:documentation>
					</xs:annotation></xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>


	<!-- GetCardTrackingHistoryRequest Element Defined -->
	<xs:element name="GetCardTrackingHistoryRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		Bulk Induction Web Service of the IDMS to retrieve the
        		Card Tracking History for a given set of search parameters.
        		Note that the results will be filtered to those events which
        		meet ALL the specified criteria.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="TransactionHeader"
						type="TransactionHeaderType"  minOccurs="1">
					<xs:annotation><xs:documentation> 
						The standard IDMS transaction header.
						</xs:documentation></xs:annotation>
				</xs:element>
				<xs:element name="CardBarCodeData" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>
							If present, this element limits the card tracking
							history results to a particular card identified by
							scanning its bar code.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="256"></xs:maxLength>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PackageTrackingID" maxOccurs="1"
					minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							If present, this element limits the card tracking
							history results to the card(s) shipped in the 
							package with the given Package tracking number 
							or other package tracking identifier.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="128"></xs:maxLength>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CardReceivedLocation" type="xs:string"
					maxOccurs="1" minOccurs="0">
                    <xs:annotation>
                    	<xs:documentation>
	                    	If present, this element limits the card tracking
							history results to the card(s) received at the
							given facility.
                    	</xs:documentation>
                    </xs:annotation>
				</xs:element>
				<xs:element name="CardHandlerPI" type="PersonIdType" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							If present, this element limits the card tracking
							history results to the card(s) handled by the
							person with the given PI.
						</xs:documentation>
					</xs:annotation></xs:element>
				<xs:element name="StartDate" type="xs:date" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							If present, this element limits the card tracking
							history results to events which occurred on or after
							the given start date.
						</xs:documentation>
					</xs:annotation></xs:element>
				<xs:element name="EndDate" type="xs:date" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							If present, this element limits the card tracking
							history results to events which occurred on or before
							the given end date.
						</xs:documentation>
					</xs:annotation></xs:element>
			</xs:sequence>
        </xs:complexType>
	</xs:element>
	
	
	
	<!-- CardTrackingHistoryEventType Element Defined -->
	<xs:complexType name="CardTrackingHistoryEventType">
        <xs:annotation>
        	<xs:documentation>
        		This type defines what information is associated with a 
        		Card Tracking History Event.
        	</xs:documentation>
        </xs:annotation>
		<xs:sequence>
			<xs:element name="CardBarCodeData" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						This element contains the data retrieved by
						scanning the bar code on a PIV card.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PackageTrackingID" maxOccurs="1"
				minOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Package tracking number or other package
						tracking identifier of the package the
						card(s) were received in.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="128"></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CardReceivedLocation" type="xs:string"
				maxOccurs="1" minOccurs="1">
                   <xs:annotation>
                   	<xs:documentation>
                   		The location of the facility where the card
                   		was received.
                   	</xs:documentation>
                   </xs:annotation>
			</xs:element>
			<xs:element name="CardHandlerPI" type="PersonIdType" maxOccurs="1" minOccurs="1">
				<xs:annotation>
					<xs:documentation>
						The PI of the person who handled the
						receipt and processing of the card.
					</xs:documentation>
				</xs:annotation></xs:element>
		</xs:sequence>
	</xs:complexType>
	
	
	<!-- GetCardTrackingHistoryResponse Element Defined -->
	<xs:element name="GetCardTrackingHistoryResponse">
        <xs:annotation>
        	<xs:documentation>
        		In response to a "GetCardTrackingHistoryRequest", the IDMS will
        		send this element to the requester so that the results
        		of the request can be retrieved.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="ResultCode" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							A short code describing the results of the
							request.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Success">
								<xs:annotation><xs:documentation>
										The query completed without errors (this doesn't guarantee that
										there will be any HistoryEvent's returned)..
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Failed">
								<xs:annotation><xs:documentation>
										An unexpected error has occurred
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="HistoryEvent" type="CardTrackingHistoryEventType" 
					minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>
							For each matching Card Tracking History Event, one of these
							History Event elements will be returned.
						</xs:documentation>
					</xs:annotation></xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	
	
	<!-- CardRenewalRequiredRequest Element Defined -->
	<xs:element name="CardRenewalRequiredRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		Bulk Induction Web Service of the IDMS in order to notify
        		the IDMS that a card renewal is required for a person.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="TransactionHeader"
						type="TransactionHeaderType"  minOccurs="1">
					<xs:annotation><xs:documentation> 
						The standard IDMS transaction header.
						</xs:documentation></xs:annotation>
				</xs:element>
				<xs:element name="CardHolderPI" type="PersonIdType"
					maxOccurs="1" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The PI of the person whose card needs to be
							renewed.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CardExpirationDate" type="xs:date" maxOccurs="1" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The date on which the card will expire.
						</xs:documentation>
					</xs:annotation></xs:element>
				<xs:element name="CardPersonalizationLocationType"
					type="CardPersonalizationLocationTypeCodeType" maxOccurs="1"
					minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							Define whether the reissued card should be
							personalized locally or at a remote printing
							facility.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NewBiometricCaptureRequired" type="xs:boolean" maxOccurs="1" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							This element indicates if the person
							receiving the card renewal needs their
							biometric data captured again prior to
							card renewal.
						</xs:documentation>
					</xs:annotation></xs:element>
			</xs:sequence>
        </xs:complexType>
	</xs:element>


	<!-- CardRenewalRequiredResponse Element Defined -->
	<xs:element name="CardRenewalRequiredResponse">
        <xs:annotation>
        	<xs:documentation>
        		In response to a "CardRenewalRequiredRequest", the IDMS will
        		send this element to the requester so that the results
        		of the request can be retrieved.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="ResultCode" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							A short code describing the results of the
							request.
						</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Success">
								<xs:annotation><xs:documentation>
										The operation was successfully processed by the IDMS.
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CardHolderPiNotFound">
								<xs:annotation><xs:documentation>
										The specified card holder is not in the system
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="BadExpirationDate">
								<xs:annotation><xs:documentation>
										The requested expiration date is in the past or
										otherwise invalid.
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="CardNotRenewable">
								<xs:annotation><xs:documentation>
										The card can not be renewed.  See the
										FailureDetailMessage element for details.
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="Failed">
								<xs:annotation><xs:documentation>
										An unexpected error has occurred
									</xs:documentation> </xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="FailureDetailMessage" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							If the ResultCode is "Failed", this field
							may contain some extra information which may
							be helpful in diagnosing the reason for the
							failure. Note that the information in this
							field is not intended to be useful to the
							average user but is intended to help
							advanced users or help desk personal debug
							the problem.
						</xs:documentation>
					</xs:annotation></xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>



	<xs:simpleType name="CardReissueReasonCodeType">
