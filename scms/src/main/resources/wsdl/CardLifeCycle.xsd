	<xs:element name="CreateBadgeRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		IDMS in order to request PIV card issuance for a PIV
        		applicant whose work flow does not include automatic card issuance.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
