	<xs:simpleType name="PIVTerminationReasonCodeType">
		<xs:annotation>
			<xs:documentation>
				This type defines the enumeration of PIV card termination /
				suspension reasons
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="EmploymentTerminated"></xs:enumeration>
			<xs:enumeration value="CardLost"></xs:enumeration>
			<xs:enumeration value="CardStolen"></xs:enumeration>
			<xs:enumeration value="Other"></xs:enumeration>
		</xs:restriction>
	</xs:simpleType>



	<!-- PIVTerminationRequest Element Defined -->
	<xs:element name="PIVTerminationRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		Bulk Induction Web Service of the IDMS in order to notify
        		the IDMS that a PIV card termination is required for a person.
        		If the person has not yet been issued a PIV card, the card
        		issuance process flow will be terminated.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="TransactionHeader"
						type="TransactionHeaderType"  minOccurs="1">
					<xs:annotation><xs:documentation> 
						The standard IDMS transaction header.
						</xs:documentation></xs:annotation>
				</xs:element>
				<xs:element name="CardHolderPI" type="PersonIdType"
					maxOccurs="1" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The PI of the person whose card needs to be
							terminated.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PIVTerminationReason"
					type="PIVTerminationReasonCodeType" maxOccurs="1"
					minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The reason code explaining why the PIV card must
							be terminated.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SuspendCardUntil" type="xs:date" maxOccurs="1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>
							If this element is specified, the PIV card will
							not be terminated immediately. Rather, the
							card will be suspended until the given date
							and then terminated.  If the PIV card has not yet 
							been issued, the card issuance process flow will 
							be suspended.
							
							If the given date is in the past, the card will 
							be terminated immediately or the card issuance process
							flow will be terminated immediately.
						</xs:documentation>
					</xs:annotation></xs:element>
			</xs:sequence>
        </xs:complexType>
	</xs:element>


	<!-- PIVTerminationResponse Element Defined -->
	<xs:element name="PIVTerminationResponse">
        <xs:annotation>
        	<xs:documentation>
        		In response to a "PIVTerminationRequest", the IDMS will
        		send this element to the requester so that the results
        		of the request can be retrieved.
        	</xs:documentation>
        </xs:annotation>
