	<!-- PIVReactivationRequest Element Defined -->
	<xs:element name="PIVReactivationRequest">
        <xs:annotation>
        	<xs:documentation>
        		A document containing this element should be sent to the
        		Bulk Induction Web Service of the IDMS in order to notify
        		the IDMS that a persons PIV card that was previously suspended
        		should be reactivated.
        		If the person had not yet been issued a PIV card at the
        		time of suspension, the card issuance process flow will 
        		continue where it had been stopped when the suspension occurred.
        	</xs:documentation>
        </xs:annotation>
        <xs:complexType>
			<xs:sequence>
				<xs:element name="TransactionHeader"
						type="TransactionHeaderType"  minOccurs="1">
					<xs:annotation><xs:documentation> 
						The standard IDMS transaction header.
						</xs:documentation></xs:annotation>
				</xs:element>
				<xs:element name="CardHolderPI" type="PersonIdType"
					maxOccurs="1" minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The PI of the person whose card needs to be
							reactivated.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PIVReactivationReason"
					type="PIVReactivationReasonCodeType" maxOccurs="1"
					minOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The reason code explaining why the PIV card is
							being reactivated.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
        </xs:complexType>
	</xs:element>
	
